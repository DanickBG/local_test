            "project": "ndr-code-bi-dashboard",
            "rawQuery": true,
            "rawSql": "SELECT \r\n  -- time,\r\n  FORMAT_TIMESTAMP('%d.%m | %H:%M Uhr', TIMESTAMP(time)) AS time,\r\n  videoViews,\r\n  reach,\r\n  type AS Typ,\r\n  -- REPLACE(message, 'ÔøΩ', '') AS Post, \r\n  CASE \r\n    WHEN LENGTH(REPLACE(SPLIT(message, '\\n')[SAFE_OFFSET(0)], 'ÔøΩ', '')) > 120 \r\n    THEN CONCAT(LEFT(REPLACE(SPLIT(message, '\\n')[SAFE_OFFSET(0)], 'ÔøΩ', ''), 120), '...')\r\n    ELSE REPLACE(SPLIT(message, '\\n')[SAFE_OFFSET(0)], 'ÔøΩ', '')\r\nEND AS Post,\r\n  CASE\r\n    WHEN type = 'Reel' THEN totalInteractions\r\n    ELSE engagement\r\n  END AS engagement,FROM `ndr-code-bi-dashboard.Quintly.instagramInsightsOwnPosts`\r\nWHERE profileId = $profileID\r\nAND (type = 'Video' OR type = 'Reel')\r\nAND $Zeitraum \r\nAND last_update = \r\n  (SELECT MAX(last_update) \r\n  FROM `ndr-code-bi-dashboard.Quintly.instagramInsightsOwnPosts` \r\n  WHERE profileId = $profileID)\r\nORDER BY reach DESC LIMIT 7\r\n",
