        "project": "ndr-code-bi-dashboard",
        "rawQuery": true,
        "rawSql": "WITH current_value AS (\r\nSELECT SUM(IFNULL(m_visits,0)) as current_sum \r\nFROM Piano.total_visits \r\nWHERE site_level2 = \"$LRA\" AND\r\n\"$Redaktionen\" = page_chapter1 AND\r\n\"$Unterkapitel\" = page_chapter2 AND\r\n\"$Sendung\" = sendungs_id \r\nAND $Zeitraum \r\n\r\n), past_value AS (\r\nSELECT SUM(IFNULL(m_visits,0)) as past_sum \r\nFROM Piano.total_visits \r\nWHERE site_level2 = \"$LRA\" AND\r\n\"$Redaktionen\" = page_chapter1 AND\r\n\"$Unterkapitel\" = page_chapter2 AND\r\n\"$Sendung\" = sendungs_id \r\nAND $Vergleichszeitraum \r\n\r\n)\r\nSELECT ((current_sum-past_sum)/past_sum)*100 FROM current_value,past_value as relative_difference",
