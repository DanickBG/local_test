        "project": "ndr-code-bi-dashboard",
        "rawQuery": true,
        "rawSql": "WITH current_value AS (\r\nSELECT \r\n  SUM(m_visits) as current_sum\r\nFROM Piano.distro\r\nWHERE  \r\nsite_level2 = \"$LRA\"\r\nAND page_chapter1 = \"$Redaktionen\"\r\nAND page_chapter2 = \"$Unterkapitel\"\r\nAND sendungs_id = \"$Sendung\"\r\nAND $Zeitraum\r\nAND src NOT LIKE '%N/A%'\r\n), past_value AS (\r\nSELECT \r\n  SUM(m_visits) as past_sum\r\nFROM Piano.distro\r\nWHERE  \r\nsite_level2 = \"$LRA\"\r\nAND page_chapter1 = \"$Redaktionen\"\r\nAND page_chapter2 = \"$Unterkapitel\"\r\nAND sendungs_id = \"$Sendung\"\r\nAND $Vergleichszeitraum \r\nAND src NOT LIKE '%N/A%'\r\n)\r\nSELECT CONCAT(REPLACE(CAST(ROUND(((current_sum-past_sum)/past_sum)*100,1) as string), \".\", \",\"), \"%\") FROM current_value,past_value as relative_difference",
