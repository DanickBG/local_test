        "location": "europe-west3",
        "project": "ndr-code-bi-dashboard",
        "rawQuery": true,
        "rawSql": "SELECT\r\n    CASE\r\n    WHEN length(content_titel) > 90 THEN SUBSTR(content_titel, 0 , 90) || '...'\r\n    ELSE content_titel\r\n  END AS content_titel, sum(m_visits) as visits \r\nFROM\r\n  (\r\n    SELECT\r\n      --publishing_date,\r\n      FIRST_VALUE(publishing_date) OVER (PARTITION BY uuid ORDER BY publishing_date DESC) as publishing_date, \r\n      FIRST_VALUE(content_id_sophora) OVER (PARTITION BY uuid ORDER BY publishing_date DESC) as content_id_sophora, \r\n      m_visits,\r\n      m_page_loads,\r\n      FIRST_VALUE(content_titel) OVER ( PARTITION BY uuid ORDER BY publishing_date DESC) as content_titel\r\n    FROM\r\n      `ndr-code-bi-dashboard.Piano.articles_joined_${dataset_vergleichszeitraum:raw}`\r\n    WHERE\r\n      site_level2 = '${app_name}'\r\n      AND content_id_sophora NOT LIKE '%index%'\r\n      AND content_id_sophora NOT LIKE '%N/A%'\r\n      AND (\r\n        CASE \r\n            WHEN \"$LRA\" = 'NDR' THEN\r\n                content_titel NOT LIKE '%Video Livestream%'\r\n            AND content_titel NOT LIKE '%Startteaser%'\r\n            AND content_titel NOT LIKE '%startteaser%'\r\n            AND content_titel NOT LIKE '%-Anmelden%'\r\n            AND content_titel NOT LIKE '%_Start'\r\n            AND content_titel != 'Anmelden'\r\n            AND content_titel != 'Jetzt l√§uft'\r\n        ELSE\r\n          TRUE\r\n        END)\r\n      AND date = ${bq_today:doublequote}\r\n  )\r\nGROUP BY\r\n  content_id_sophora,\r\n  content_titel,\r\n  publishing_date\r\nORDER BY\r\n  2 DESC\r\nLIMIT\r\n  10",
