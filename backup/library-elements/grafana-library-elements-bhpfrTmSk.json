        "project": "ndr-code-bi-dashboard",
        "rawQuery": true,
        "rawSql": "SELECT\n    CASE\n    WHEN length(content_titel) > 90 THEN SUBSTR(content_titel, 0 , 90) || '...'\n    ELSE content_titel\n  END AS content_titel,\n  -- content_id_sophora,\n  sum(m_visits) as visits\nFROM\n  (\n    SELECT\n      --publishing_date,\n      FIRST_VALUE(publishing_date) OVER (\n        PARTITION BY uuid\n        ORDER BY\n          publishing_date DESC\n      ) as publishing_date,\n      FIRST_VALUE(content_id_sophora) OVER (\n        PARTITION BY uuid\n        ORDER BY\n          publishing_date DESC\n      ) as content_id_sophora,\n      m_visits,\n      m_page_loads,\n      FIRST_VALUE(content_titel) OVER (\n        PARTITION BY uuid\n        ORDER BY\n          publishing_date DESC\n      ) as content_titel\n    FROM\n      `ndr-code-bi-dashboard.Piano.articles_joined_${dataset_zeitraum:raw}`\n    WHERE\n      site_level2 = '${app_name}'\n      AND content_id_sophora NOT LIKE '%index%'\n      AND content_id_sophora NOT LIKE '%N/A%'\n      AND (\n        CASE\n          WHEN \"$LRA\" = 'NDR' THEN content_titel NOT LIKE '%Video Livestream%'\n          AND content_titel NOT LIKE '%Startteaser%'\n          AND content_titel NOT LIKE '%startteaser%'\n          AND content_titel NOT LIKE '%-Anmelden%'\n          AND content_titel NOT LIKE '%_Start'\n          AND content_titel != 'Anmelden'\n          AND content_titel != 'Jetzt l√§uft'\n          ELSE TRUE\n        END\n      )\n      AND date = ${bq_today:doublequote}\n    )\nGROUP BY\n  content_id_sophora,\n  content_titel,\n  publishing_date\nORDER BY\n  visits DESC\nLIMIT\n  10;\n",
