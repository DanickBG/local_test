                        "project": "ndr-code-bi-dashboard",
                        "rawQuery": true,
                        "rawSql": "WITH nachrichten AS (\n  SELECT\n    CASE\n      page_chapter2\n      WHEN \"thueringen\" THEN \"Th\u00fcringen\"\n      WHEN \"sachsen\" THEN \"Sachsen\"\n      WHEN \"sachsen-anhalt\" THEN \"Sachsen-Anhalt\"\n      WHEN \"deutschland\" THEN \"Deutschland\"\n      WHEN \"welt\" THEN \"Welt\"\n    END as Chapter,\n    SUM(COALESCE(m_visits, 0)) as summe\n  FROM\n    `Piano.total_visits${dataset_zeitraum:raw}`\n  WHERE\n    page_chapter1 = \"nachrichten\"\n    AND page_chapter2 IN (\n      \"sachsen\",\n      \"sachsen-anhalt\",\n      \"thueringen\",\n      \"deutschland\",\n      \"welt\"\n    )\n    AND site_level2 = \"$LRA\"\n    AND sendungs_id = \"$Sendung\"\n    AND $Zeitraum\n  GROUP BY\n    Chapter\n),\nratgeber AS(\n  SELECT\n    \"Ratgeber\" as Chapter,\n    SUM(COALESCE(m_visits, 0)) as summe\n  FROM\n    `Piano.total_visits${dataset_zeitraum:raw}`\n  WHERE\n    page_chapter1 = \"ratgeber\"\n    AND site_level2 = \"$LRA\"\n    AND page_chapter2 = \"$Unterkapitel\"\n    AND sendungs_id = \"$Sendung\"\n    AND $Zeitraum\n  GROUP BY\n    Chapter --), app_aktuell AS (\n    --  SELECT\n    --  \"App Aktuell\" as Chapter,\n    --  SUM(COALESCE(m_visits,0)) as summe\n    --  FROM Piano.total_visits\n    --  WHERE page_chapter1 = \"nachrichten\" AND page_chapter2 IN (\"app_aktuell\",\"app-aktuell\") AND site_level2 = \"$LRA\" AND sendungs_id = \"$Sendung\" AND $Zeitraum\n),\nndr_kapitel as (\n  SELECT\n    CASE\n      page_chapter1\n      WHEN \"nachrichten\" THEN \"Nachrichten\"\n      WHEN \"kultur\" THEN \"Kultur\"\n      WHEN \"sport\" THEN \"Sport\"\n    END as Chapter,\n    SUM(COALESCE(m_visits, 0)) as summe\n  FROM\n    `Piano.total_visits${dataset_zeitraum:raw}`\n  WHERE\n    site_level2 = \"$LRA\"\n    AND page_chapter1 IN (\"nachrichten\", \"kultur\", \"sport\")\n    AND page_chapter2 = \"$Unterkapitel\"\n    AND sendungs_id = \"$Sendung\"\n    AND $Zeitraum\n  GROUP BY\n    Chapter\n),\nmdr as(\n  SELECT\n    *\n  FROM\n(\n      SELECT\n        Chapter,\n        summe\n      FROM\n        nachrichten\n      UNION\n      ALL\n      SELECT\n        Chapter,\n        summe\n      FROM\n        ratgeber --UNION ALL \n        --SELECT\n        --Chapter,\n        --summe FROM app_aktuell\n    )\n)\nSELECT\n  DISTINCT CASE\n    WHEN \"$LRA\" = \"ndr.de\" THEN ndr_kapitel.Chapter\n    WHEN \"$LRA\" = \"mdr.de\" THEN mdr.Chapter\n  END as Chapter,\n  CASE\n    WHEN \"$LRA\" = \"ndr.de\" THEN ndr_kapitel.summe\n    WHEN \"$LRA\" = \"mdr.de\" THEN mdr.summe\n  END as summe\nFROM\n  mdr,\n  ndr_kapitel\nORDER BY\n  summe desc\n\n",
                        "refId": "N-Joy Zeitraum",
